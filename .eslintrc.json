{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "off",
      "no-undef": "off",
      "no-param-reassign": "off",
      "no-await-in-loop": "off",
      "no-restricted-syntax": "off",
      "consistent-return": "off"
    }
}
